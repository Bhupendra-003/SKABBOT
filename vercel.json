{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": [
        "cdg1"
    ],
    "github": {
        "silent": true
    },
    "REWRITES": [
        {
            "SOURCE": "/(.*)",
            "DESTINATION": "/"
        }
    ]
}